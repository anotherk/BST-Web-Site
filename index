$(document).ready(function(){
	//escondemos los submenus cuando se carga el archivo
	$('#menu ul li ul').hide();	
	//se activa cuando el usuario pasa el cursor encima del menu
	$('#menu ul li').hover(
		
  		function(){
			//mantiene en posicion los submenus
			$('#menu ul li').not($('ul', this)).stop();
    		//desliza hacia abajo los submenus
			$('ul', this).slideDown('fast');
  		}, 
		//deliza hacia arriba los submenus
  		function(){
    		$('ul', this).slideUp('fast');
  		}
	);

});// ]]>
</script>

